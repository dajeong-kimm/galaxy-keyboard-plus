    "@modelcontextprotocol/server-google-maps": "^0.6.2",
    "@modelcontextprotocol/server-memory": "^0.6.2",
    "@modelcontextprotocol/server-postgres": "^0.6.2",
    "@modelcontextprotocol/server-puppeteer": "^2025.4.7",
    "@modelcontextprotocol/server-redis": "^2025.4.7",
    "@modelcontextprotocol/server-sequential-thinking": "^0.6.2",
