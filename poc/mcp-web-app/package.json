{
  "name": "mcp-web-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^35.1.5",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "cors": "^2.8.5"
  },
  "build": {
    "appId": "com.mcp.webapp",
    "productName": "MCP Web App",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist/",
      "!node_modules/.cache/"
    ]
  }
}
